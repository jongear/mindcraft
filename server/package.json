{
  "name": "mindcraft-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "npm test -- --watch",
    "lint": "eslint ."
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ]
  },
  "dependencies": {
    "@tensorflow/tfjs-node": "^0.3.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "colors": "^1.3.0",
    "cookie-parser": "^1.4.3",
    "csv-parse": "^4.3.3",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "minidrone": "^1.1.0",
    "morgan": "^1.9.0",
    "node-thinkgear-sockets": "0.0.1",
    "openbci": "^3.0.0",
    "socket.io": "^2.4.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.1",
    "jest": "^22.4.4",
    "prettier": "^1.13.6"
  }
}
